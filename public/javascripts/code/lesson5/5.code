// Счетчик скорости корабля.
var speed = 0;

// Результат выполнения инкремента.
var result;

// В результате получим 0.
result = ++speed;
BBotDebug("Результат операции: " + result);
BBotDebug("Скорость корабля: " + speed);

result = speed++;
BBotDebug("Результат операции: " + result);
BBotDebug("Скорость корабля: " + speed);
