// Счетчик скорости корабля.
var speed = 0;

// Результат выполнения инкремента.
var result;

// Результат выполнения операции: 1.
result = ++speed;
BBotDebug("Результат операции: " + result); // 1
BBotDebug("Скорость корабля: " + speed);	// 1

// Результат выполнения операции: 1.
result = speed++;
BBotDebug("Результат операции: " + result); // 1
BBotDebug("Скорость корабля: " + speed);    // 2
