{
	"!name": "spacecraft",
	"!define": {
		"SpaceCraft": {
			"!doc": "Корабль, управляемый пилотом.",
			"moveForward": {
				"!type": "fn()",
				"!doc": "Отправляет корабль вперед."
			},
			"rotateLeft": {
				"!type": "fn()",
				"!doc": "Поворот корабля налево."
			},
			"rotateRight": {
				"!type": "fn()",
				"!doc": "Поворот корабля направо."
			},
			"moveToXY": {
				"!type": "fn(x: number, y: number)",
				"!doc": "Отправляет корабль к заданным координатам."
			},
			"scan": {
				"!type": "fn()",
				"!doc": "Сканирует область вокруг корабля."
			}
		}
	},
	"BBotDebug": {
		"!type": "fn(text: string)",
		"!doc": "BBotDebug - команда вывода текста на экран BBot'а. Она поможет вам анализировать код, который вы написали."
	},
	"run": {
		"!type": "fn(spaceCraft: +SpaceCraft)",
		"!doc": "Функция run в бесконечном цикле последовательно, раз за разом, выполняет написанный код управления. Являтеся одной из самых важных функций SpaceCraft."
	}
}
